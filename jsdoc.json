{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": "src/main/js",
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "opts": {
    "template": "node_modules/docdash",
    "encoding": "utf8",
    "destination": "docs/",
    "recurse": true,
    "verbose": true
  },
  "markdown": {
    "parser": "gfm",
    "hardwrap": true,
    "idInHeadings": true
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "docdash": {
    "static": false,
    "sort": true,
    "search": true,
    "collapse": true,
    "wrap": true,
    "typedefs": true,
    "private": true
  }
}
